{
    "version": 2,
    "builds": [
      {
        "src": "api/src/server.ts", 
        "use": "@vercel/node"
      },
      {
        "src": "frontend/package.json", 
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/src/server.ts" 
      },
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      }
    ]
  }
  